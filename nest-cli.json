{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": ["assets/**/*"]
  },
  "plugins": [
    {
      "name": "@nestjs/graphql/plugin",
      "options": {
        "typeFileNameSuffix": [".args.ts", ".dto.ts", ".input.ts", ".model.ts"]
      }
    }
  ]
}
